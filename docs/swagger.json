                    {
                        "type": "integer",
                        "default": 20,
                        "description": "Jumlah data per halaman",
                        "name": "pageSize",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/snacks": {
            "get": {
                "description": "Mengambil seluruh daftar snack yang tersedia",
