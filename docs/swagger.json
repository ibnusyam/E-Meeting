{
    "swagger": "2.0",
    "info": {
        "description": "Ini adalah API server untuk aplikasi E-Meeting.",
        "title": "E-Meeting API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "ini",
    "paths": {
        "/profile/:id": {
            "get": {
                "description": "Mengambil data profile berdasarkan id",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Profile"
                ],
                "summary": "Mendapatkan data profile",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.ProfileUser"
                            }
                        }
                    }
                }
            }
        },
        "/snacks": {
            "get": {
                "description": "Mengambil seluruh daftar snack yang tersedia",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Snacks"
                ],
                "summary": "Mendapatkan semua data snack",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.Snack"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.ProfileUser": {
            "type": "object",
            "properties": {
                "createdAt": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "imageURL": {
                    "type": "string"
                },
                "language": {
                    "type": "string"
                },
                "phone_number": {
                    "type": "string"
                },
                "role": {
                    "type": "string"
                },
                "status_user": {
                    "type": "string"
                },
                "updatedAt": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "model.Snack": {
            "type": "object",
            "properties": {
                "category": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "price": {
                    "type": "number"
                }
            }
        }
    }
}